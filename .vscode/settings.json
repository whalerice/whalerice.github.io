{
  "cSpell.words": [
    "adsense",
    "iconify",
    "lucide",
    "nuxi",
    "nuxt",
    "persistedstate",
    "rawbody",
    "shiki",
    "sundrycrew",
    "vueuse",
    "whalerice"
  ],
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
